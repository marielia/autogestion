<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">
<faces-config>
 <converter>
  <converter-id>BigDecimal</converter-id>
  <converter-class>java.math.BigDecimal</converter-class>
 </converter>
 <converter>
  <converter-id>BigInteger</converter-id>
  <converter-class>java.math.BigInteger</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>logonBean</managed-bean-name>
  <managed-bean-class>com.asecor.extranet.faces.seguridad.LogonBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
  <managed-bean>
  	<managed-bean-name>userRegisterBean</managed-bean-name>
  	<managed-bean-class>com.asecor.extranet.faces.seguridad.UserRegisterBean</managed-bean-class>
  	<managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  	<managed-bean-name>registrationConfirmationBean</managed-bean-name>
  	<managed-bean-class>com.asecor.extranet.faces.seguridad.RegistrationConfirmationBean</managed-bean-class>
  	<managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  	<managed-bean-name>forgotYouPasswordBean</managed-bean-name>
  	<managed-bean-class>com.asecor.extranet.faces.seguridad.ForgotYouPasswordBean</managed-bean-class>
  	<managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  	<managed-bean-name>cleanPasswordBean</managed-bean-name>
  	<managed-bean-class>com.asecor.extranet.faces.seguridad.CleanPasswordBean</managed-bean-class>
  	<managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  	<managed-bean-name>logonSelBean</managed-bean-name>
  	<managed-bean-class>com.asecor.extranet.faces.seguridad.LogonSelBean</managed-bean-class>
  	<managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  	<managed-bean-name>logoFirstBean</managed-bean-name>
  	<managed-bean-class>com.asecor.extranet.faces.seguridad.LogoFirstBean</managed-bean-class>
  	<managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  	<managed-bean-name>logoInternoBean</managed-bean-name>
  	<managed-bean-class>com.asecor.extranet.faces.seguridad.LogoInternoBean</managed-bean-class>
  	<managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  	<managed-bean-name>consultaPoliza</managed-bean-name>
  	<managed-bean-class>com.asecor.extranet.faces.polizas.ConsultaPolizas</managed-bean-class>
  	<managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
   <managed-bean>
  <managed-bean-name>editUser</managed-bean-name>
  <managed-bean-class>com.asecor.extranet.faces.seguridad.EditarTitularWeb</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  
   <navigation-case>
   <from-outcome>bienvenido</from-outcome>
   <to-view-id>/pages/web/app/bienvenida.jsp</to-view-id>
  </navigation-case>
 
    <navigation-case>
   <from-outcome>polizasUsuarios</from-outcome>
   <to-view-id>/pages/web/app/listado_polizas.xhtml</to-view-id>
  </navigation-case>
    <navigation-case>
   <from-outcome>validarEmail</from-outcome>
   <to-view-id>/pages/web/app/validarEmail.xhtml</to-view-id>
  </navigation-case>
  <!--
   <navigation-case>
   <from-outcome>polizasUsuarios</from-outcome>
   <to-view-id>/pages/web/app/inicio.jsp</to-view-id>
  </navigation-case> -->
   <navigation-case>
   <from-outcome>registrar</from-outcome>
   <to-view-id>/pages/web/app/registracion.xhtml</to-view-id>
  </navigation-case>
   <navigation-case>
   <from-outcome>indexTemp</from-outcome>
   <to-view-id>/pages/web/indexTemp.jsp</to-view-id>
  </navigation-case>
  
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/pages/web/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>cambio_password</from-outcome>
   <to-view-id>/pages/web/app/cambio_password.xhtml</to-view-id>
  </navigation-case>
  </navigation-rule>
 <application>
  <locale-config>
   <default-locale>es_AR</default-locale>
  </locale-config>
  <message-bundle>com.asecor.extranet.bundle.Messages_es_AR</message-bundle>
 </application>
